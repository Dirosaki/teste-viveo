{
	"plugins": [
		"import-helpers"
	],
	"rules": {
		"import-helpers/order-imports": [
			"error",
			{
				"newlinesBetween": "always",
				"groups": [
					"/^react$|react-dom|react-router-dom/",
					"/^styled-components/",
					"module",
					"/^services/",
					"/^contexts/",
					"/^providers/",
					"/^routes/",
					"/^hooks/",
					"/^reducers/",
					"/^store/",
					"/^errors/",
					"/^layouts/",
					"/^pages/",
					"/^components/",
					"/^utils/",
					"/^data/",
					"/^constants/",
					"/^assets/",
					"/^types|^@types/",
					"/^styles/",
					[
						"parent",
						"sibling"
					],
					"index",
					"/^./styles/"
				],
				"alphabetize": {
					"order": "asc",
					"ignoreCase": true
				}
			}
		]
	}
}
